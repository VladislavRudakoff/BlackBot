{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CachingSettings": {
    "CacheTtl": "0.00:10:00"
  },
  "ConnectionStrings": {
    "Redis": "localhost:6379,defaultDatabase=1",
    "PostgreSQL": "Server=<DefaultServer>;Port=5555;Database=<MyDatabase>;User Id=<MyUserId>;Password=<MyPassword>;Integrated Security=true;Integrated Security=true;Pooling=true;"
  },
  "TelegramClientSettings": {
    "Token": "<DefaultToken>",
    "BotName": "MrSelectorBot",
    "BotUrl": "https://t.me/MrSelectorBot",
    "BaseUrl": "https://api.telegram.org/bot{0}/",
    "TimeOut": "0.00:01:00",
    "BaseArgs": "",
    "WebHookUrl": ""
  },
  "AllowedHosts": "*"
}
